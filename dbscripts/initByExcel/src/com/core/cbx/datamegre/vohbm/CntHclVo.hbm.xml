<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>

    <class name = "com.core.cbx.datamegre.vo.cnt.CntHclVo" table="CNT_HCL">

            <id name="id" ><generator class="native" /></id>
            <property name="revision" type="java.lang.Integer"><column name="REVISION"  sql-type="NUMBER(9,0)" /> </property>
            <property name="entityVersion" type="java.lang.Integer"><column name="ENTITY_VERSION"  sql-type="NUMBER(9,0)" /> </property>
            <property name="domainId" type="string"><column name="DOMAIN_ID"  sql-type="NVARCHAR2(400)" /> </property>
            <property name="status" type="string"><column name="STATUS"  sql-type="NVARCHAR2(400)" /> </property>
            <property name="docStatus" type="string"><column name="DOC_STATUS"  sql-type="NVARCHAR2(400)" /> </property>
            <property name="edittingStatus" type="string"><column name="EDITING_STATUS"  sql-type="NVARCHAR2(400)" /> </property>
            <property name="createUser" type="string"><column name="CREATE_USER"  sql-type="NVARCHAR2(120)" /> </property>
            <property name="updateUser" type="string"><column name="UPDATE_USER"  sql-type="NVARCHAR2(120)" /> </property>
            <property name="createdOn" type="date"><column name="CREATED_ON"  sql-type="TIMESTAMP" /> </property>
            <property name="updatedOn" type="date"><column name="UPDATED_ON"  sql-type="TIMESTAMP" /> </property>
            <property name="hclTypeId" type="string"><column name="HCL_TYPE_ID"  sql-type="CHAR(32)" /> </property>
            <property name="name" type="string"><column name="NAME"  sql-type="NVARCHAR2(400)" /> </property>
            <property name="description" type="string"><column name="DESCRIPTION"  sql-type="NVARCHAR2(4000)" /> </property>
    </class>

</hibernate-mapping>
